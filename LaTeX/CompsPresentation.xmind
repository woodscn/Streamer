\section{The Unified Coordinates Method}

\subsection{Theoretical Background}

\begin{frame}{The Unified Coordinate Transformation}

\begin{align}
\label{eq:coordinate_transformation}
\left(
\begin{array}{c}
dt\\dx\\dy\\dz
\end{array}
\right) &= \left(
\begin{array}{cccc}
1 & 0 & 0 & 0 \\
U & A & L & P \\
V & B & M & Q \\
W & C & N & R \end{array} \right)
\left( \begin{array}{c}
d\lambda\\d\xi\\d\eta\\d\zeta \end{array} \right)
\end{align}

\end{frame}

\begin{frame}{Conservation Equations}
\begin{equation}
\label{eq:conservation}
\frac{\partial \bf{E}}{\partial \lambda} + \frac{\partial \bf{F}}{\partial \xi} + \frac{\partial \bf{G}}{\partial \eta} = 0
\end{equation}
{\small
\begin{equation}
\label{eq:vectors}
\bf{E} = \left(
\begin{array}{c}
\rho \Delta \\
\rho \Delta u \\
\rho \Delta v \\
\rho \Delta e \\
A \\
B \\
L \\
M 
\end{array}
\right)
,
\bf{F} = \left(
\begin{array}{c}
\rho \left( 1 - h \right) I \\
\rho \left( 1 - h \right) I u + p M \\
\rho \left( 1 - h \right) I v - p L \\
\rho \left( 1 - h \right) I e + p I \\
- h u \\
- h v \\
0 \\
0
\end{array}
\right)
,
\bf{G} = \left(
\begin{array}{c}
\rho \left( 1 - h \right) J \\
\rho \left( 1 - h \right) J u - p B \\
\rho \left( 1 - h \right) J v + p A \\
\rho \left( 1 - h \right) J e + p J \\
0 \\
0 \\
- h u \\
- h v
\end{array}
\right)
\end{equation}
}
\begin{equation}
\label{eq:intermediates}
\Delta = A M - B L , \,I = u M - v L , \,J = A v - B u
\end{equation}

\end{frame}

