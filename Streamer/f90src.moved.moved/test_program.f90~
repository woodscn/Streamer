program test_regime
  use generalutilitiestest
  use godunov_tester
  use riemann_tester
  implicit none
  integer :: result, junk
  write(*,*) "General Utilities test:"
  result = GUTest()
  junk = GUErrorReader(result)
  write(*,*) "Riemann tester:"
  result = RieTester()
  junk = RieErrorReader(result)
  write(*,*) "Godunov tester:"
  result = GodTester()
  junk = GodErrorReader(result)
